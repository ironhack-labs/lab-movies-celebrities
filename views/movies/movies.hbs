<h1>{{title}}</h1>
<h2>MOVIES</h2>
<hr>

<div class="container">
    <div class="row">

        {{#each movie}}
        <h3><a href="/movies/{{_id}}">{{title}}</a></h3>
        <form action="/movies/{{_id}}/delete" method="post">
            <button class="btn btn-danger mb-3" type="submit">Delete</button>
        </form>
        <a href="/movies/{{_id}}/edit">edit</a>
        {{/each}}
    </div>
</div>