<h2>Details of the movie:</h2>
<p>Title: {{title}}</p>
<p>Genre: {{genre}}</p>
<p>Plot: {{plot}}</p>
<p>Cast:</p>
{{#each cast}}
  <ul>
    <li>Name: {{name}}</li>
    <li>Occupation: {{occupation}}</li>
    <li>Catch Phrase: {{catchPhrase}}</li>
  </ul>
{{/each}}
<form action="/movies/{{id}}/delete" method="POST">
<button type="submit">Delete Movie</button>
</form>
<form action="/movies/{{id}}/edit" method="GET">
<button type="submit">Edit Movie</button>
</form>
