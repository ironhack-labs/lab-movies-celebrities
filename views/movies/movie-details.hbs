<h2>{{newMovie.title}}</h2> 
<p>{{newMovie.description}}</p>
<p>{{newMovie.plot}}</p>
{{#each newMovie.cast}}
    <li>
        <ul>{{name}}</ul>
        <ul>{{occupation}}</ul>
        <ul>{{catchPhrase}}</ul>
    </li>
{{/each}}

<form action="/movies/{{newMovie._id}}/delete" method="POST">
  <button value="DELETE">DELETE</button>
</form>

<form action="/movies/{{newMovie._id}}/edit" method="GET">
  <button value="EDIT">EDIT</button>
</form>