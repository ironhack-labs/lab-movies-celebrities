<h2>Movie Details</h2>
<p>Title: {{title}}</p>
<p>Genre: {{genre}}</p>
<p>Plot: {{plot}}</p>
{{#each cast}}
  <p>{{this.name}}</p>
  <p>{{this.occupation}}</p>
  <p>{{this.catchPhrase}}</p>
{{/each}}
<form action='/movies/{{_id}}/delete' method='post'>
  <button>Delete</button>
  <a href='/movies/{{_id}}/edit'>Edit</a>
</form>