<h2>Movie</h2>
<p>{{movie.title}}</p>
<p>{{movie.genre}}</p>
<p>{{movie.plot}}</p>
{{#each movie.cast}}
    <p>{{name}}</p>
    <p>{{occupation}}</p>
    <p>{{catchPhrase}}</p>
{{/each}}
<a href="/movies/{{movie._id}}/edit">Edit the Movie</a>
<form action="/movies/{{movie._id}}/delete" method="post">
    <button type="submit">Delete</button>
</form>