<div class="container">
    <h2>Movie details</h2>
    <p>{{title}}</p>
    <p>{{genre}}</p>
    <p>{{plot}}</p>
    {{#each celebrity}}
    <p>{{name}}</p>
    <p>{{occupation}}</p>
    <p>{{catchPhrase}}</p>
    {{/each}}
    <form action="/movies/movies/{{_id}}/delete" method="POST">
        <input type="submit" value="DELETE!">
    </form>
    <a href="/movies/movies/{{_id}}/edit">Edit</a>

</div>