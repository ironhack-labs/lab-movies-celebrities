<div class="row justify-content-center mt-5">
    <hr>
    {{#each celebrities}}
    <div class="col-4 text-center mb-3 mt-5">
        <div class="card" style="width: 20rem; background-color:beige">
            <div class="card-body">
                <h5 class="card-title">{{name}}</h5>
                <a href="/celebrities/{{_id}}/edit" class="btn btn-danger" style="color:yellow">Edit</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>