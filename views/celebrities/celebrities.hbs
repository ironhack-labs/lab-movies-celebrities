<div class="container">

    <h1>Listado de Celebrities</h1>

    <div class="row">

        {{#each celebrities}}

        <div class="col-md-4">

            <div class="card" style="width: 18rem;">

                <div class="card-body">

                    <h5 class="card-title">{{name}}</h5>

                </div>
            </div>

        </div>

        {{/each}}

    </div>
</div>